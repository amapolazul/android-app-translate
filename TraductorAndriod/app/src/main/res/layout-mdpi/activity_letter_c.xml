<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="40dp"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.under_six.letters.letter_c.LetterCActivity"
android:background="@drawable/fondo202"
android:id="@+id/letterCActivityLayout">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/caballo"
    android:src="@drawable/caballo"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playHorseSound"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/cabeza"
    android:src="@drawable/cabeza"
    android:onClick="playHeadFaSound"
    android:layout_alignTop="@+id/caballo"
    android:layout_toRightOf="@+id/caballo"
    android:layout_toEndOf="@+id/caballo"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/cafe"
    android:onClick="playCoffeeBallSound"
    android:layout_alignTop="@+id/cabeza"
    android:layout_centerHorizontal="true"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dentista"
    android:src="@drawable/cama"
    android:onClick="playBedSound"
    android:layout_alignTop="@+id/delfin"
    android:layout_toRightOf="@+id/delfin"
    android:layout_toEndOf="@+id/delfin"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/deporte"
    android:src="@drawable/camisa"
    android:onClick="playShirtSound"
    android:layout_alignTop="@+id/dentista"
    android:layout_toRightOf="@+id/dentista"
    android:layout_toEndOf="@+id/dentista" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/cara"
    android:onClick="playFaceRoomSound"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/carro"
    android:onClick="playCarSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/cabeza"
    android:layout_alignStart="@+id/cabeza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/cepillo"
    android:onClick="playBrushSound"
    android:layout_alignTop="@+id/dia"
    android:layout_toRightOf="@+id/delfin"
    android:layout_toEndOf="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToUnderSixCLetter"
    android:layout_alignTop="@+id/masFichasD"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dia"
    android:src="@drawable/casa"
    android:onClick="playHousePortSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/delfin"
    android:layout_alignStart="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/cerdo"
    android:onClick="playPigSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/deporte"
    android:layout_alignStart="@+id/deporte" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/masFichasD"
    android:src="@drawable/fichas4"
    android:clickable="true"
    android:layout_alignParentTop="true"
    android:layout_alignRight="@+id/deporte"
    android:layout_alignEnd="@+id/deporte"
    android:onClick="goToOtherWordsCLetter" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>

