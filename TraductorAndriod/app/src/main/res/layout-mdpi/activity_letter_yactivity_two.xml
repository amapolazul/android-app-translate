<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="40dp"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.under_six.letters.letter_y.LetterYActivityTwo"
android:background="@drawable/fondo202"
android:id="@+id/letterYActivityLayoutTwo">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diez"
    android:src="@drawable/yodo"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playIodineSound"
    android:layout_marginRight="35dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dinero"
    android:src="@drawable/yogur"
    android:onClick="playYogurtSound"
    android:layout_alignTop="@+id/diente"
    android:layout_toRightOf="@+id/diente"
    android:layout_toEndOf="@+id/diente"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/yuca"
    android:onClick="playYuccaSound"
    android:layout_alignTop="@+id/dinero"
    android:layout_toRightOf="@+id/direccion"
    android:layout_toEndOf="@+id/direccion" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/direccion"
    android:src="@drawable/yoyo"
    android:onClick="playYoyoSound"
    android:layout_alignTop="@+id/delfin"
    android:layout_toRightOf="@+id/dinero"
    android:layout_toEndOf="@+id/dinero"
    android:layout_marginRight="18dp" />

    <ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/doctor"
    android:src="@drawable/yugular"
    android:onClick="playJugularSound"
        android:layout_alignTop="@+id/dos"
        android:layout_toRightOf="@+id/dos"
        android:layout_toEndOf="@+id/dos"
        android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dos"
    android:src="@drawable/yugo"
    android:onClick="playYokeSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/dados"
    android:layout_toEndOf="@+id/dados"
    android:layout_marginRight="18dp" />

    <ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToYLetter"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/domir"
    android:src="@drawable/yunque"
    android:onClick="playAnvilSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/doctor"
    android:layout_toEndOf="@+id/doctor" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/yoga"
    android:onClick="playYogaSound"
    android:layout_alignTop="@+id/diez"
    android:layout_toRightOf="@+id/dos"
    android:layout_toEndOf="@+id/dos"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>
