<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.under_six.letters.letter_v.LetterVActivityTwo"
android:background="@drawable/fondo202"
android:id="@+id/letterOActivityLayoutTwo">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diez"
    android:src="@drawable/verde"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playGreenSound"
    android:layout_marginRight="35dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dinero"
    android:src="@drawable/viajar"
    android:onClick="playTravelSound"
    android:layout_alignTop="@+id/diente"
    android:layout_alignLeft="@+id/domir"
    android:layout_alignStart="@+id/domir" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/vino"
    android:onClick="playWineSound"
    android:layout_alignTop="@+id/dinero"
    android:layout_toRightOf="@+id/direccion"
    android:layout_toEndOf="@+id/direccion" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/direccion"
    android:src="@drawable/vida"
    android:onClick="playLifeSound"
    android:layout_alignTop="@+id/delfin"
    android:layout_toRightOf="@+id/domir"
    android:layout_toEndOf="@+id/domir"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dulce"
    android:src="@drawable/volcan"
    android:onClick="playVolcanoSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/domir"
    android:layout_toEndOf="@+id/domir" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/doctor"
    android:src="@drawable/violin"
    android:onClick="playViolinSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/diez"
    android:layout_toEndOf="@+id/diez" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dos"
    android:src="@drawable/violeta"
    android:onClick="playVioletSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/dados"
    android:layout_toEndOf="@+id/dados" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/durazno"
    android:src="@drawable/volumen"
    android:onClick="playVolumeSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/delfin"
    android:layout_alignStart="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToVLetter"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/domir"
    android:src="@drawable/volar"
    android:onClick="playFlySound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/imageView35"
    android:layout_alignStart="@+id/imageView35"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/vestido"
    android:onClick="playDressSound"
    android:layout_alignTop="@+id/diez"
    android:layout_toRightOf="@+id/diez"
    android:layout_toEndOf="@+id/diez" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>

