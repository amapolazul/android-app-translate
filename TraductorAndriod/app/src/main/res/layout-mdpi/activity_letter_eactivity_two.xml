<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="40dp"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.LetterEActivityTwo"
android:background="@drawable/fondo202"
android:id="@+id/letterDActivityLayoutTwo">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diez"
    android:src="@drawable/ensalada"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playSaladSound"
    android:layout_marginRight="35dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dinero"
    android:src="@drawable/escoba"
    android:onClick="playBroomSound"
    android:layout_alignTop="@+id/diente"
    android:layout_alignLeft="@+id/domir"
    android:layout_alignStart="@+id/domir"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/espaguetis"
    android:onClick="playSpaghettiSound"
    android:layout_alignTop="@+id/dinero"
    android:layout_toRightOf="@+id/dulce"
    android:layout_toEndOf="@+id/dulce" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/direccion"
    android:src="@drawable/escritorio"
    android:onClick="playDeskSound"
    android:layout_alignTop="@+id/delfin"
    android:layout_toRightOf="@+id/domir"
    android:layout_toEndOf="@+id/domir"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dulce"
    android:src="@drawable/estrella"
    android:onClick="playStarSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/domir"
    android:layout_toEndOf="@+id/domir"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/doctor"
    android:src="@drawable/espejo"
    android:onClick="playMirrorSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/dos"
    android:layout_toEndOf="@+id/dos" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dos"
    android:src="@drawable/espalda"
    android:onClick="playBackSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/dados"
    android:layout_toEndOf="@+id/dados"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/durazno"
    android:src="@drawable/estufa"
    android:onClick="playStoveSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/delfin"
    android:layout_alignStart="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToELetter"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/domir"
    android:src="@drawable/esqueleto"
    android:onClick="playSkeletonSound"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true"
    android:layout_marginRight="18dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/escalera"
    android:onClick="playStairSound"
    android:layout_alignTop="@+id/diez"
    android:layout_toRightOf="@+id/dos"
    android:layout_toEndOf="@+id/dos" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>



