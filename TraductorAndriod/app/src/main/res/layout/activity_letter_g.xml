<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.under_six.letters.letter_g.LetterGActivity"
android:background="@drawable/fondo202"
android:id="@+id/letterGActivityLayout">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dados"
    android:src="@drawable/gafas"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playGlasesSound"
    android:layout_marginRight="35dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dedo"
    android:src="@drawable/galletas"
    android:onClick="playCookiesSound"
    android:layout_alignTop="@+id/dados"
    android:layout_toRightOf="@+id/dados"
    android:layout_toEndOf="@+id/dados" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/gallina"
    android:onClick="playHenSound"
    android:layout_alignTop="@+id/dentista"
    android:layout_alignLeft="@+id/diamante"
    android:layout_alignStart="@+id/diamante"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dentista"
    android:src="@drawable/garaje"
    android:onClick="playGarageSound"
    android:layout_alignTop="@+id/dedo"
    android:layout_toRightOf="@+id/delfin"
    android:layout_toEndOf="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/deporte"
    android:src="@drawable/gaseosa"
    android:onClick="playSodaSound"
    android:layout_alignTop="@+id/dentista"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_marginRight="50dp"
    android:layout_marginEnd="50dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/desayuno"
    android:src="@drawable/gasolina"
    android:onClick="playGasolineSound"
    android:layout_alignTop="@+id/dia"
    android:layout_toLeftOf="@+id/cabeza"
    android:layout_toStartOf="@+id/cabeza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dia"
    android:src="@drawable/gato"
    android:onClick="playCatSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/dados"
    android:layout_toEndOf="@+id/dados" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diario"
    android:src="@drawable/gente"
    android:onClick="playPeopleSound"
    android:layout_alignParentBottom="true"
    android:layout_toRightOf="@+id/diamante"
    android:layout_toEndOf="@+id/diamante" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToUnderSixGLetter"
    android:layout_alignTop="@+id/masFichasD"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diamante"
    android:src="@drawable/gelatina"
    android:onClick="playJellySound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/imageView35"
    android:layout_alignStart="@+id/imageView35"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/gimnasio"
    android:onClick="playGymSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/deporte"
    android:layout_alignStart="@+id/deporte" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/masFichasD"
    android:src="@drawable/fichas4"
    android:clickable="true"
    android:layout_alignParentTop="true"
    android:layout_alignRight="@+id/deporte"
    android:layout_alignEnd="@+id/deporte"
    android:onClick="goToOtherWordsGLetter" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>

