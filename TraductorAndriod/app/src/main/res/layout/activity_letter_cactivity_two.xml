<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="8dp"
android:paddingTop="5dp"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="com.amapolazul.www.traductorandriod.under_six.letters.letter_c.LetterCActivityTwo"
android:background="@drawable/fondo202"
android:id="@+id/letterCActivityLayoutTwo">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/cereza"
    android:src="@drawable/cereza"
    android:layout_below="@+id/imageView35"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="34dp"
    android:onClick="playCherrySound" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/cabeza"
    android:src="@drawable/cinco"
    android:onClick="playFiveSound"
    android:layout_marginLeft="37dp"
    android:layout_marginStart="37dp"
    android:layout_alignTop="@+id/cereza"
    android:layout_toRightOf="@+id/cereza"
    android:layout_toEndOf="@+id/cereza"
    android:layout_marginRight="34dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/delfin"
    android:src="@drawable/cocina"
    android:onClick="playkitchenBallSound"
    android:layout_alignTop="@+id/cabeza"
    android:layout_alignLeft="@+id/dia"
    android:layout_alignStart="@+id/dia" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dentista"
    android:src="@drawable/colegio"
    android:onClick="playSchoolSound"
    android:layout_marginLeft="42dp"
    android:layout_marginStart="42dp"
    android:layout_alignTop="@+id/delfin"
    android:layout_toRightOf="@+id/delfin"
    android:layout_toEndOf="@+id/delfin" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/deporte"
    android:src="@drawable/comida"
    android:onClick="playFoodSound"
    android:layout_alignTop="@+id/dentista"
    android:layout_toRightOf="@+id/dentista"
    android:layout_toEndOf="@+id/dentista"
    android:layout_marginLeft="40dp"
    android:layout_marginStart="40dp" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/computador"
    android:onClick="playComputerSound"
    android:layout_alignParentBottom="true"
    android:layout_toLeftOf="@+id/cabeza"
    android:layout_toStartOf="@+id/cabeza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/conejo"
    android:onClick="playRabbitSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/cabeza"
    android:layout_alignStart="@+id/cabeza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/cuatro"
    android:onClick="playFourSound"
    android:layout_alignTop="@+id/diente"
    android:layout_toLeftOf="@+id/deporte"
    android:layout_toStartOf="@+id/deporte" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/volverPrincipalD"
    android:src="@drawable/fichas3"
    android:onClick="returnToLetterCActivity"
    android:layout_alignRight="@+id/cereza"
    android:layout_alignEnd="@+id/cereza" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/dia"
    android:src="@drawable/cuadro"
    android:onClick="playPictureSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/imageView35"
    android:layout_alignStart="@+id/imageView35" />

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/diente"
    android:src="@drawable/cuchara"
    android:onClick="playSpoonSound"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/deporte"
    android:layout_alignStart="@+id/deporte" />

    <ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView35"
    android:src="@drawable/fichas2"
    android:layout_alignTop="@+id/volverPrincipalD"
    android:layout_centerHorizontal="true" />
</RelativeLayout>

